<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Settings"
    android:id="@+id/relativeLayout3">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="246dp"
        android:layout_height="31dp"
        android:layout_marginTop="178dp"
        android:text="Enable outgoing notifications"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="180dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="246dp"
        android:layout_height="31dp"
        android:layout_marginTop="225dp"
        android:text="Customize outgoing"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="240dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="246dp"
        android:layout_height="31dp"
        android:layout_marginTop="127dp"
        android:text="Customize incoming"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="121dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="246dp"
        android:layout_height="31dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:text="Notifications"
        android:textSize="24dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="246dp"
        android:layout_height="31dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="78dp"
        android:text="Enable incoming notifications"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="66dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/manageZones"
        android:layout_width="251dp"
        android:layout_height="41dp"
        android:layout_marginBottom="176dp"
        android:layout_marginStart="4dp"
        android:text="Manage Zones"
        android:textSize="24dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView6" />

    <TextView
        android:id="@+id/newZone"
        android:layout_width="237dp"
        android:layout_height="33dp"
        android:layout_marginBottom="119dp"
        android:text="New Zone"
        android:textSize="17sp"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintBottom_toBottomOf="parent" />

    <TextView
        android:id="@+id/editZone"
        android:layout_width="241dp"
        android:layout_height="30dp"
        android:layout_marginBottom="72dp"
        android:text="Edit Zones"
        android:textSize="17sp"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintBottom_toBottomOf="parent" />

    <Switch
        android:id="@+id/enableIncomingSwitch"
        android:layout_width="57dp"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintTop_toTopOf="@+id/textView12"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <Switch
        android:id="@+id/enableOutgoingSwitch"
        android:layout_width="57dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintStart_toEndOf="@+id/textView7" />






    <ImageButton
        android:id="@+id/customizeIncomingButton"
        android:layout_width="59dp"
        android:layout_height="36dp"
        android:background="@drawable/colorbutton"
        android:textColor="@drawable/button_text_color"
        app:srcCompat="@drawable/incoming_notifications"
        app:layout_constraintTop_toTopOf="@+id/textView8"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <ImageButton
        android:id="@+id/customizeOutgoingButton"
        android:layout_width="61dp"
        android:layout_height="37dp"
        android:background="@drawable/colorbutton"
        android:textColor="@drawable/button_text_color"
        app:srcCompat="@drawable/customize_outgoing"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <ImageButton
        android:id="@+id/newZoneButton"
        android:layout_width="63dp"
        android:layout_height="37dp"
        android:background="@drawable/colorbutton"
        android:textColor="@drawable/button_text_color"
        app:srcCompat="@drawable/add_zone"
        app:layout_constraintBottom_toBottomOf="@+id/newZone"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <ImageButton
        android:id="@+id/editZoneButton"
        android:layout_width="65dp"
        android:layout_height="36dp"
        android:background="@drawable/colorbutton"
        android:textColor="@drawable/button_text_color"
        app:srcCompat="@drawable/edit_zones"
        app:layout_constraintTop_toTopOf="@+id/editZone"
        app:layout_constraintStart_toEndOf="@+id/textView7" />


</android.support.constraint.ConstraintLayout>